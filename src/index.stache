<html>
  <head>
    <title>{{title}}</title>
  </head>
  <body>
    <can-import from="bootstrap/less/bootstrap.less" />
    <can-import from="donejs-chat/styles.less" />
    <can-import from="donejs-chat/app" export-as="viewModel" route-data="routeData" />

    <div class="container">
      <div class="row">
        <div class="col-sm-8 col-sm-offset-2">
          {{#if(pageComponent.isResolved)}}
            {{pageComponent.value}}
          {{else}}
            Loading...
          {{/if}}
        </div>
      </div>
    </div>

    {{#switch env.NODE_ENV}}
      {{#case "production"}}
        <script src="{{joinBase 'steal.production.js'}}"></script>
      {{/case}}
      {{#default}}
        <script src="/node_modules/steal/steal.js" main></script>
      {{/default}}
    {{/switch}}
  </body>
</html>
