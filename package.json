{
  "name": "donejs-chat",
  "version": "0.0.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "scripts": {
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 8080",
    "develop": "done-serve --develop --port 8080",
    "document": "documentjs",
    "build": "node build"
  },
  "main": "production.html",
  "files": [
    "src"
  ],
  "keywords": [],
  "steal": {
    "main": "donejs-chat/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "https://donejs-chat.firebaseapp.com/"
      }
    },
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ]
  },
  "dependencies": {
    "bit-tabs": "^1.0.0-alpha.0",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "can-component": "^3.0.0",
    "can-connect": "^1.0.0",
    "can-define": "^1.0.0",
    "can-list": "^3.0.0",
    "can-map": "^3.0.0",
    "can-map-define": "^3.0.0",
    "can-route": "^3.0.0",
    "can-route-pushstate": "^3.0.0",
    "can-zone": "^0.6.0",
    "done-autorender": "^1.0.0-alpha.0",
    "done-component": "^0.6.0-pre.4",
    "done-css": "~3.0.0-alpha.0",
    "done-serve": "^1.0.0-alpha.0",
    "express-ie-cors": "^0.9.4",
    "feathers": "^1.1.1",
    "feathers-hooks": "^0.5.1",
    "feathers-memory": "^0.3.4",
    "generator-donejs": "^0.8.0",
    "jquery": "~2.2.1",
    "steal": "^1.0.0",
    "steal-less": "^1.0.2",
    "steal-socket.io": "^2.0.0",
    "steal-stache": "^3.0.0"
  },
  "devDependencies": {
    "can-fixture": "^1.0.0",
    "documentjs": "^0.4.2",
    "donejs-cli": "^0.8.0",
    "donejs-deploy": "^0.4.0",
    "funcunit": "~3.0.0",
    "steal-cordova": "^0.1.3",
    "steal-nw": "^0.1.4",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.0",
    "testee": "^0.2.4"
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "firebase",
          "config": {
            "firebase": "donejs-chat",
            "public": "./dist",
            "headers": [
              {
                "source": "/**",
                "headers": [
                  {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                  }
                ]
              }
            ]
          }
        }
      }
    }
  },
  "window": {
    "toolbar": false,
    "width": 1060,
    "height": 625
  },
  "engines": {
    "node": "5.8"
  }
}
